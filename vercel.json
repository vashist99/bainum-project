{
    "version": 2,
    "outputDirectory": "dist",
    "builds": [
      {
        "src": "backend/src/server.js",
        "use": "@vercel/node"
      },
      {
        "src": "mockup1/package.json",
        "use": "@vercel/static-build",
        "config": { "distDir": "dist" }
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/backend/src/server.js"
      },
      {
        "source": "/(.*)",
        "destination": "/mockup1/index.html"
      }
    ]
  }
  